{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Gaze Prediction",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/atari_head/gaze_predictor.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--debug",
                "--load_model",
                "--n", "1",
                "--eval_train_data",
                "--load_saliency"
            ]
        },{
            "name": "OG Agent",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/agents/dqn_atari_cr/main.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--clip_reward",
                "--capture_video",
                "--exp_name", "test_og",
                "--total_timesteps", "5000",
                "--learning_start", "100",
                "--eval_num", "3",
                "--no_pvm_visualization",
                "--no_model_output",
                "--disable_tensorboard"
            ]
        },
        {
            "name": "Ray",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/hyperparams.py",
            "console": "integratedTerminal",
        },
        {
            "name": "ViT",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/atari_head/vit.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--debug",
                "--load_saliency"
            ]
        },
        {
            "name": "DQN CR Sugarl",
            "type": "debugpy",
            "request": "launch",
            "program": "src/atari_cr/agents/dqn_atari_cr/main.py",
            "console": "integratedTerminal",
            "pythonArgs": [
                // Not having this displays a warning because frozen modules bad
                "-Xfrozen_modules=off"
            ],
            "args": [
                "--clip_reward",
                "--capture_video",
                "--env", "ms_pacman",
                // Debug config
                "--exp_name", "dqn_cr_debug",
                "--total_timesteps", "10000",
                "--learning_start", "1000",
                "--debug",
                // Additional config
                "--pause_cost", "0.05",
                "--use_pause_env",
                "--action_repeat", "5"
            ],
            "justMyCode": false
        },
    ]
}